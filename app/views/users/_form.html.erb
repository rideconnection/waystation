<%= form_for @user do |f| %>
  <%= f.error_messages %>
  <p>
    <%= f.label :username %><br />
    <%= f.text_field :username %>
  </p>
  <p>
    <%= f.label :email %><br />
    <%= f.text_field :email %>
  </p>
  <p>
    <%= f.label :encrypted_password %><br />
    <%= f.text_field :encrypted_password %>
  </p>
  <p>
    <%= f.label :reset_password_token %><br />
    <%= f.text_field :reset_password_token %>
  </p>
  <p>
    <%= f.label :reset_password_sent_at %><br />
    <%= f.datetime_select :reset_password_sent_at %>
  </p>
  <p>
    <%= f.label :remember_created_at %><br />
    <%= f.datetime_select :remember_created_at %>
  </p>
  <p>
    <%= f.label :sign_in_count %><br />
    <%= f.text_field :sign_in_count %>
  </p>
  <p>
    <%= f.label :current_sign_in_at %><br />
    <%= f.datetime_select :current_sign_in_at %>
  </p>
  <p>
    <%= f.label :last_sign_in_at %><br />
    <%= f.datetime_select :last_sign_in_at %>
  </p>
  <p>
    <%= f.label :current_sign_in_ip %><br />
    <%= f.text_field :current_sign_in_ip %>
  </p>
  <p>
    <%= f.label :last_sign_in_ip %><br />
    <%= f.text_field :last_sign_in_ip %>
  </p>
  <p>
    <%= f.label :password_salt %><br />
    <%= f.text_field :password_salt %>
  </p>
  <p>
    <%= f.label :confirmation_token %><br />
    <%= f.text_field :confirmation_token %>
  </p>
  <p>
    <%= f.label :confirmed_at %><br />
    <%= f.datetime_select :confirmed_at %>
  </p>
  <p>
    <%= f.label :confirmation_sent_at %><br />
    <%= f.datetime_select :confirmation_sent_at %>
  </p>
  <p>
    <%= f.label :failed_attempts %><br />
    <%= f.text_field :failed_attempts %>
  </p>
  <p>
    <%= f.label :unlock_token %><br />
    <%= f.text_field :unlock_token %>
  </p>
  <p>
    <%= f.label :locked_at %><br />
    <%= f.datetime_select :locked_at %>
  </p>
  <p>
    <%= f.label :authentication_token %><br />
    <%= f.text_field :authentication_token %>
  </p>
  <p><%= f.submit %></p>
<% end %>
